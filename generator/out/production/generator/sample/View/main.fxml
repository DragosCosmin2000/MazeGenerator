<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="750.0" prefWidth="1000.0" style="-fx-background-color: white;" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.Controller.JavaFXController">
    <children>
      <Pane layoutX="375.0" layoutY="650.0" prefHeight="75.0" prefWidth="600.0" style="-fx-border-color: #7544ae; -fx-border-radius: 5; -fx-border-width: 3;">
         <children>
              <Button fx:id="startStopButton" layoutX="50.0" layoutY="20.0" mnemonicParsing="false" onAction="#startStopAnimation" prefHeight="35.0" prefWidth="100.0" text="%startStopButtonText" />
            <Button fx:id="normalSpeedButton" layoutX="192.0" layoutY="20.0" mnemonicParsing="false" onAction="#setToNormalSpeed" prefHeight="35.0" prefWidth="35.0" text="x1">
               <font>
                  <Font name="System Bold" size="12.0" />
               </font>
            </Button>
            <Button layoutX="269.0" layoutY="20.0" mnemonicParsing="false" onAction="#setTox2Speed" prefHeight="35.0" prefWidth="35.0" text="x2" fx:id="x2SpeedButton">
               <font>
                  <Font name="System Bold" size="12.0" />
               </font>
            </Button>
            <Button layoutX="346.0" layoutY="20.0" mnemonicParsing="false" onAction="#setTox4Speed" prefHeight="35.0" prefWidth="35.0" text="x4" fx:id="x4SpeedButton">
               <font>
                  <Font name="System Bold" size="12.0" />
               </font>
            </Button>
            <Button layoutX="423.0" layoutY="20.0" mnemonicParsing="false" onAction="#setTox8Speed" prefHeight="35.0" prefWidth="35.0" text="x8" fx:id="x8SpeedButton">
               <font>
                  <Font name="System Bold" size="12.0" />
               </font>
            </Button>
            <Button fx:id="maxSpeedButton" layoutX="500.0" layoutY="20.0" mnemonicParsing="false" onAction="#setToMaxSpeed" prefHeight="35.0" prefWidth="50.0" text="MAX">
               <font>
                  <Font name="System Bold" size="12.0" />
               </font>
            </Button>
         </children>
      </Pane>
        <Button fx:id="backButton" layoutX="25.0" layoutY="550.0" mnemonicParsing="false" onAction="#backToMenu" prefHeight="75.0" prefWidth="325.0" style="-fx-alignment: center; -fx-background-color: white; -fx-border-radius: 5; -fx-border-color: #7544ae; -fx-border-width: 3;" text="%backButtonText" textAlignment="CENTER" textFill="#7544ae">
            <font>
                <Font name="System Bold" size="30.0" />
            </font>
        </Button>
        <Button fx:id="saveButton" layoutX="25.0" layoutY="25.0" mnemonicParsing="false" onAction="#saveMaze" prefHeight="75.0" prefWidth="325.0" style="-fx-background-color: white; -fx-border-color: #7544ae; -fx-border-width: 3; -fx-alignment: center; -fx-border-radius: 5;" text="%saveButtonText" textAlignment="CENTER" textFill="#7544ae">
            <font>
                <Font name="System Bold" size="30.0" />
            </font></Button>
        <Button fx:id="exitButton" layoutX="25.0" layoutY="650.0" mnemonicParsing="false" onAction="#exitApp" prefHeight="75.0" prefWidth="325.0" style="-fx-background-color: white; -fx-border-color: #7544ae; -fx-border-width: 3; -fx-border-radius: 5; -fx-alignment: center;" text="%exitButtonText" textAlignment="CENTER" textFill="#7544ae">
            <font>
                <Font name="System Bold" size="30.0" />
            </font></Button>
        <Pane fx:id="mazePane" layoutX="375.0" layoutY="25.0" prefHeight="600.0" prefWidth="600.0" style="-fx-background-color: white;" />
        <Button fx:id="saveAsJsonButton" layoutX="25.0" layoutY="125.0" mnemonicParsing="false" onAction="#saveMazeAsJson" prefHeight="75.0" prefWidth="325.0" style="-fx-background-color: white; -fx-border-color: #7544ae; -fx-border-width: 3; -fx-alignment: center; -fx-border-radius: 5;" text="%saveAsJsonButtonText" textAlignment="CENTER" textFill="#7544ae">
            <font>
                <Font name="System Bold" size="30.0" />
            </font>
        </Button>

    </children>
</AnchorPane>
